{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sron",
      "camelcase": "SRON",
      "title": "SRInternet-Object Notation",
      "scope": "source.Srd",
      "file-types": [
        "Srd"
      ],
      "injection-regex": "^sron$",
      "class-name": "TreeSitterSron"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Sron grammar for tree-sitter",
    "authors": [
      {
        "name": "Pizero",
        "email": "zhaory200707@outlook.com",
        "url": "https://github.com/pizerolol"
      }
    ],
    "links": {
      "repository": "https://github.com/pizerolol/tree-sitter-sron"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}